            }
            if ($('#COurltext_' + week + '_' + mod_num).val() == '') {
                $('#COurltext_' + week + '_' + mod_num).addClass('active')
            } else {
                if (/^(http|https|ftp):\/\/[a-z0-9]+([\-\.]{1}[a-z0-9]+)*\.[a-z]{2,5}(:[0-9]{1,5})?(\/.*)?$/i.test($('#COurltext_' + week + '_' + mod_num).val())) {
